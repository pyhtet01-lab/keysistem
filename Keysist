G2L["8"].Position = UDim2.new(0.92593, 0, 0, 0)

-- –ö–ª—é—á–∏
local validKeys = {
    ["G1T7XQ4Mkey9A2Z8K5R0N6"] = true,
    ["fgftop1"] = true,
}

-- –ö–æ–ø–∏—Ä–æ–≤–∞–Ω–∏–µ Discord —Å—Å—ã–ª–∫–∏
G2L["5"].MouseButton1Click:Connect(function()
    setclipboard("https://discord.gg/ttQMyKvKt6")
    G2L["5"].Text = "Copied!"
    task.delay(1.5, function()
        G2L["5"].Text = "Copy link key"
    end)
end)

-- –ü—Ä–æ–≤–µ—Ä–∫–∞ –∫–ª—é—á–∞
G2L["6"].MouseButton1Click:Connect(function()
    local input = G2L["3"].Text
    if validKeys[input] then
        G2L["6"].Text = "Access Granted!"
        -- –ó–¥–µ—Å—å –º–æ–∂–µ—à—å –∞–∫—Ç–∏–≤–∏—Ä–æ–≤–∞—Ç—å –æ—Å–Ω–æ–≤–Ω–æ–π GUI –∏–ª–∏ –∑–∞–≥—Ä—É–∑–∏—Ç—å —Å–∫—Ä–∏–ø—Ç
    else
        G2L["6"].Text = "Invalid Key"
    end
    task.delay(2, function()
        G2L["6"].Text = "LoadKey"
    end)
end)

-- –ó–∞–∫—Ä—ã—Ç–∏–µ GUI
G2L["8"].MouseButton1Click:Connect(function()
    G2L["1"]:Destroy()
end)

-- –¶–µ–Ω—Ç—Ä–∏—Ä—É–µ–º GUI
G2L["2"].AnchorPoint = Vector2.new(0.5, 0.5)
G2L["2"].Position = UDim2.new(0.5, 0, 0.5, 0)

-- –î–µ–ª–∞–µ–º GUI –ø–æ–¥–≤–∏–∂–Ω—ã–º –º—ã—à–∫–æ–π
local dragging, dragInput, dragStart, startPos

G2L["7"].InputBegan:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseButton1 then
        dragging = true
        dragStart = input.Position
        startPos = G2L["2"].Position
        input.Changed:Connect(function()
            if input.UserInputState == Enum.UserInputState.End then
                dragging = false
            end
        end)
    end
end)

G2L["7"].InputChanged:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseMovement then
        dragInput = input
    end
end)

game:GetService("UserInputService").InputChanged:Connect(function(input)
    if input == dragInput and dragging then
        local delta = input.Position - dragStart
        G2L["2"].Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)
    end
end)

return G2L["1"], require
